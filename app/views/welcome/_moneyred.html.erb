<% unless current_user %>
    <% if t.ten_fee < t.one_fee %>
      <%= number_to_currency(t.ten_fee,precision: 2 ) %>~<%= number_to_currency(t.one_fee, precision: 2) %>
      USD/Hour
    <% else %>
      <%= number_to_currency(t.one_fee, precision: 2) %>~<%= number_to_currency(t.ten_fee,precision: 2 ) %>
      USD/Hour
    <% end %>
<% else %>
    <% if t.ten_fee < t.one_fee %>
      <%= number_to_currency(t.ten_fee_exchange_to(current_user.currency),precision: 2 ) %>~<%= number_to_currency(t.one_fee_exchange_to(current_user.currency), precision: 2) %>
      <%= current_user.currency %>/Hour
    <% else %>
      <%= number_to_currency(t.one_fee_exchange_to(current_user.currency), precision: 2) %>~<%= number_to_currency(t.ten_fee_exchange_to(current_user.currency),precision: 2 ) %>
      <%= current_user.currency %>/Hour
    <% end %>
<% end %>
