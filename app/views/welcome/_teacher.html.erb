<% @teachers.each do |t| %>
<% if t.check == "checked" %>
<div class="container">
  <div class="info">
    <div class="row">
      <div class="info-image col-sm-5">
        <iframe src="<%= t.youtube_website %>?playlist=&version=3&controls=0&fs=0&rel=0&showinfo=0&iv_load_policy=3&theme=light" frameborder="0" allowfullscreen></iframe>
      </div>
      <div class="col-xs-12 col-sm-7">
        <div class="col-xs-12 clearfix flexmodify">
          <div class="info-mypic col-md-2 col-xs-3">
            <%=   image_tag(User.find(t.user_id).image.url(:medium)) %>
          </div>
          <%= link_to profile_teacher_path(t)  do %>
          <div class="info-eval col-md-7 col-xs-7">
            <p><%= User.find(t.user_id).first_name %>
              <%= User.find(t.user_id).last_name %></p>
            <i class="fa fa-star" aria-hidden="true"></i>
            <i class="fa fa-star" aria-hidden="true"></i>
            <i class="fa fa-star" aria-hidden="true"></i>
            <i class="fa fa-star-half-o"></i>
            <i class="fa fa-star-o"></i>
            <span>(132)</span>
          </div>
          <div class=" col-xs-2 info-flag col-md-push-2 col-md-1">
            <%= image_tag("flag.png") %>
          </div>
        </div>
        <div class="col-xs-12 clearfix info-tag">
          <i class="fa fa-comment-o"></i>
          <span>Speak
          </span>
          <span>
            <% t.languages.each do |x| %>
            <%= x.language %>
            <% end %>
          </span><br>
          <i class="fa fa-usd"></i>
          <span>Rate
          </span>
          <span class="moneyred"><%= t.ten_fee/10 %>~<%= t.one_fee %>
          TW/hour</span><br>
          <i class="fa fa-rss"></i>
          <span>Trial
          </span>
          <span>Yes</span>
        </div>
        <div class="col-xs-12 info-pro">
          <p><%= t.introduction %></p>
        </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
<% end %>
<% end %>
