
 <table class="table table-hover">
  <% @teachers.each do |t| %>
  <tr>
      <td>
       <%= t.avg_rating %>
      </td>
      <td>
        <div id="rateYo-<%= t.id %>"></div>
      </td>
  </tr>
  <% end %>
</table>



<script type="text/javascript" charset="utf-8">

$(function () {
  <% @teachers.each do |t| %>
  $("#rateYo-<%= t.id %>").rateYo({
    starWidth: "40px",
    numStars: 5,
    halfStar: true,
    readOnly: true,
    rating: <%= t.avg_rating %>
  });
  <% end %>
});



</script>
